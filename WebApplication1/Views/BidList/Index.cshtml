@model IEnumerable<WebApplication1.Models.BidList.BidListVm>

@{
    ViewData["Title"] = "My auction overview";
}

<h1>My auction overview</h1>


<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.Title)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.UserName)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.Title)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.UserName)
            </td>
            <td>
                @Html.ActionLink("View list", "Details", new { id = item.Id})
            </td>
        </tr>
}
    </tbody>
</table>
